<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle">

    <!--thin-->
    <font
        android:font="@font/noto_sans_thin"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/noto_sans_thin"
        app:fontStyle="italic"
        app:fontWeight="300" />

    <!--regular-->
    <font
        android:font="@font/noto_sans_regular"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/noto_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--blod-->
    <font
        android:font="@font/noto_sans_bold"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/noto_sans_bold"
        app:fontStyle="normal"
        app:fontWeight="1000" />

</font-family>